{
  "name": "@strands/agent-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for building AI agents with model-driven approach",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/repl/repl.ts",
    "repl": "tsx src/repl/repl.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "bedrock",
    "mcp",
    "typescript"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.540.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  }
}